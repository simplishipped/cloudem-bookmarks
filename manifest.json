{
  "manifest_version": 3,
  "name": "blockmarks",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Blockmarks",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "webRequest",
    "cookies",
    "notifications"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}