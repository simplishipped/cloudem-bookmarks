{
  "manifest_version": 3,
  "name": "Bookmarks+",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Bookmarks+",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "activeTab",
    "bookmarks"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}